<!DOCTYPE html>
<html>
    <head>
        <%- include('head', {}); %>
    </head>
    <body>
        <%- include('header', {}); %>
        <div class="content">
            <% if (manufacturers) { %>
            <ul>
                <% manufacturers.forEach(function(manufacturer) { %>
                <li>
                    Name: <a href="/manufacturer/<%= manufacturer.id %>"><%= manufacturer.name %></a><br>
                    Description: <%= manufacturer.description %>
                </li>
                <% }); %>
            </ul>
            <% } %>
        </div>
        <%- include('footer', {}); %>
    </body>
</html>
