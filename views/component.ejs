<!DOCTYPE html>
<html>
    <head>
        <%- include('head', {}); %>
    </head>
    <body>
        <%- include('header', {}); %>
        <div class="content">
            <% if (component) { %>
            <div class="pure-g">
                <div class="pure-u-1-3">
                    <img width="80px" height="80px" src="">
                </div>
                <div class="pure-u-2-3">
                    <div>
                        <h2><%= component.manufacturerPartNumber %></h2>
                    </div>
                    <% if (manufacturer) { %>
                    <a href="/manufacturer/<%= manufacturer.id %>">
                        <div class="pure-g">
                            <div class="pure-u-1-10">
                                <img width="20px" height="20px" src="">
                            </div>
                            <div class="pure-u-1-5">
                                <%= manufacturer.name%>
                            </div>
                        </div>
                    </a>
                    <% } %>
                </div>
            </div>
            <% } %>
        </div>
        <%- include('footer', {}); %>
    </body>
</html>
