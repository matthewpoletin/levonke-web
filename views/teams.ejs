<!DOCTYPE html>
<html>
    <head>
        <%- include('head', {}); %>
    </head>
    <body>
        <%- include('header', {}); %>
        <div class="content">
            <% if (teams) { %>
            <ul>
                <% teams.forEach(function(team) { %>
                <li>
                    Name: <%= component.manufacturerPartNumber %><br>
                    Organization id: <a href="http://localhost:3000/organization/<%= organization.id %>"><%= team.organizationId%></a>
                </li>
                <% }); %>
            </ul>
            <% } %>
        </div>
        <%- include('footer', {}); %>
    </body>
</html>
